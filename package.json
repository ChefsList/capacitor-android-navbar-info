{
  "name": "system-bars",
  "version": "0.0.1",
  "description": "System bars dimentions on Android",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "SystemBars.podspec"
  ],
  "author": "Chefslist",
  "license": "Copyright by Chefslist",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/chefslist/chefslist-system-bars.git.git"
  },
  "bugs": {
    "url": "https://gitlab.com/chefslist/chefslist-system-bars.git/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:android && npm run verify:web",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "docgen": "docgen --api SystemBarsPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/docgen": "^0.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": ">=7.0.0"
  },
  "capacitor": {
    "platforms": ["android"],
    "android": {
      "src": "android"
    }
  }
}
